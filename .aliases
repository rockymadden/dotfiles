alias ......='cd ../../../../..'
alias -- -='cd -'
alias a='atom'
alias afk="/System/Library/CoreServices/Menu\ Extras/User.menu/Contents/Resources/CGSession -suspend"
alias aliases='grep alias < ~/.aliases | cut -c 7- | sort'
alias chome='chrome-cli'
alias ci='circleci'
alias cib='circleci browse'
alias cin='circleci notify'
alias cit='circleci trigger'
alias copy="tr -d '\n' | pbcopy"
alias dcs='cd ~/Documents'
alias desktop-on="defaults write com.apple.finder CreateDesktop -bool true && killall Finder"
alias desktop-off="defaults write com.apple.finder CreateDesktop -bool false && killall Finder"
alias dls='cd ~/Downloads'
alias dns-flush="sudo dscacheutil -flushcache && sudo killall -HUP mDNSResponder"
alias download='aria2c'
alias dsstore-cleanup="f() { find ${1-.} -type f -name '*.DS_Store' -ls -delete; }; f"
alias encrypt-asymmetric-transfer='gpg -ae -o - | transfer'
alias encrypt-symmetric-transfer='gpg -ac -o - | transfer'
alias g='git'
alias gcm='git cm'
alias gco='git co'
alias gd='git done'
alias ge='git eject'
alias gf='git feature'
alias gg='git graph'
alias gh='hub'
alias ghb='hub browse'
alias ghc='hub clone'
alias gla='git la'
alias gls='git ls'
alias gp='git push'
alias gpn='f() { git push && { sleep 10 ; circleci notify; }; }; f'
alias gpg-decrypt-asymmetric='gpg'
