"*":
  "atom-color-highlight": {}
  "atom-material-ui":
    colors:
      abaseColor:
        red: 33
        green: 150
        blue: 243
        alpha: 1
      predefinedColor: "Blue"
    fonts: {}
    tabs:
      compactTabs: true
      tintedTabBar: true
    treeView:
      blendTabs: true
      compactList: true
    ui:
      panelContrast: true
      panelShadows: true
  "autocomplete-plus": {}
  core:
    disabledPackages: [
      "hash-n"
      "base-n"
      "vim-mode"
    ]
    followSymlinks: true
    themes: [
      "atom-material-ui"
      "atom-material-syntax-light"
    ]
  editor:
    fontFamily: "Source Code Pro"
    invisibles: {}
    preferredLineLength: 100
    showIndentGuide: true
    showInvisibles: true
  "exception-reporting":
    userId: "3693b2ab-2f02-8f93-38ce-d016022c40b1"
  "file-watcher":
    promptWhenFileHasChangedOnDisk: false
  "git-log":
    fontFamily: "Source Code Pro"
    fontScale: 1
  "highlight-selected":
    minimumLength: 2
  linter:
    statusBar: "Show error of the selected line"
    subtleLinterErrors: [
      "jshint"
      "jsxhint"
    ]
  "linter-jshint": {}
  minimap:
    autoToggle: true
    plugins:
      "color-highlight": true
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
  "one-dark-ui":
    layoutMode: "Compact"
  "one-light-ui":
    layoutMode: "Compact"
  react: {}
  "recent-files":
    updated: false
  "tree-view":
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
".coffee.jsx.source":
  editor:
    preferredLineLength: 100
".js.jsx.source":
  editor:
    preferredLineLength: 100
